{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "app-plus": {
          "navigationBarTitleText": "首屏加载页",
          "navigationStyle": "custom",
          "animationType": "slide-in-left",
          "animationDuration": 500
        }
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "app-plus": {
          "navigationBarTitleText": "登录",
          "navigationStyle": "custom",
          "animationType": "slide-in-left",
          "animationDuration": 500
        }
      }
    },
    {
      "path": "pages/table/index",
      "style": {
        "enablePullDownRefresh": true,
        "app-plus": {
          "navigationBarTitleText": "课表",
          "navigationStyle": "custom",
          "animationType": "slide-in-left",
          "animationDuration": 500,
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/personal/index",
      "style": {
        "app-plus": {
          "navigationBarTitleText": "个人中心",
          "animationType": "slide-in-left",
          "animationDuration": 500
        }
      }
    },
    {
      "path": "pages/exam/index",
      "style": {
        "app-plus": {
          "navigationBarTitleText": "我的考试",
          "animationType": "slide-in-left",
          "animationDuration": 500
        }
      }
    }
  ],
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#1296db",
    "borderStyle": "#efe7e7",
    "backgroundColor": "#ffffff",
    "height": "65px",
    "fontSize": "14px",
    "iconWidth": "30px",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/table/index",
        "text": "课表",
        "iconPath": "static/image/tabBar/table.png",
        "selectedIconPath": "static/image/tabBar/table_selected.png"
      },
      {
        "pagePath": "pages/personal/index",
        "text": "个人中心",
        "iconPath": "static/image/tabBar/personal.png",
        "selectedIconPath": "static/image/tabBar/personal_selected.png"
      }
    ]
  },
  "globalStyle": {
    "app-plus": {
      "navigationBarTextStyle": "black",
      "navigationBarTitleText": "uni-app"
    }
  },
  "condition": {
    "current": 0,
    "list": [
      {
        "name": "",
        "path": "",
        "query": ""
      }
    ]
  }
}
