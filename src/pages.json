{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "app-plus": {
          "navigationBarTitleText": "首屏加载页",
          "navigationStyle": "custom",
          "animationType": "slide-in-right",
          "animationDuration": 500
        }
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "app-plus": {
          "navigationBarTitleText": "登录",
          "navigationStyle": "custom",
          "animationType": "slide-in-right",
          "animationDuration": 500
        }
      }
    },
    {
      "path": "pages/table/index",
      "style": {
        "navigationBarTitleText": "课表",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true,
        "app-plus": {
          "animationType": "slide-in-right",
          "animationDuration": 500
        }
      }
    },
    {
      "path": "pages/personal/index",
      "style": {
        "navigationBarTitleText": "个人中心",
        "navigationStyle": "custom",
        "app-plus": {
          "animationType": "slide-in-right",
          "animationDuration": 500
        }
      }
    }
  ],
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#1296db",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/table/index",
        "text": "课表",
        "iconPath": "static/image/table.png",
        "selectedIconPath": "static/image/table_selected.png"
      },
      {
        "pagePath": "pages/personal/index",
        "text": "个人中心",
        "iconPath": "static/image/personal.png",
        "selectedIconPath": "static/image/personal_selected.png"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",

    "backgroundColor": "#F8F8F8",
    "h5": {
      "titleNView": false
    }
  },
  "condition": {
    //模式配置，仅开发期间生效
    "current": 0, //当前激活的模式(list 的索引项)
    "list": [
      {
        "name": "", //模式名称
        "path": "", //启动页面，必选
        "query": "" //启动参数，在页面的onLoad函数里面得到
      }
    ]
  }
}
